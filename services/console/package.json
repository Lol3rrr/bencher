{
  "name": "bencher-console",
  "type": "module",
  "version": "0.4.3",
  "scripts": {
    "fmt": "npx biome format --write .",
    "typeshare": "typeshare ../.. --lang=typescript --output-file=./src/types/bencher.ts",
    "wasm": "./build_wasm.sh",
    "copy": "cp -r ../cli/templates/output ./public/download && cp ../api/swagger.json ./public/download/openapi.json",
    "setup": "npm run typeshare && npm run wasm && npm run copy",
    "dev": "npm run setup && export BENCHER_API_URL=http://127.0.0.1:61016 && export GITHUB_CLIENT_ID=Iv1.46f0fd7a007ef1af && astro dev --port 3000",
    "start": "astro dev --port 3000",
    "test": "npm run setup && vitest",
    "build": "export $(cat .env.runtime) && npm run copy && astro build",
    "node": "npm install && npm install @astrojs/node@8.1.0 && npx astro add node -y && npm run build",
    "netlify": "npm install && npm install @astrojs/netlify@5.0.0 && npx astro add netlify -y && npm run build",
    "preview": "export BENCHER_API_URL=http://127.0.0.1:61016 && export GITHUB_CLIENT_ID=Iv1.12877f7bd5e10428 && astro preview --port 3000",
    "astro": "astro",
    "check": "astro check"
  },
  "dependencies": {
    "@astrojs/check": "^0.4.1",
    "@astrojs/mdx": "^2.1.1",
    "@astrojs/partytown": "^2.0.4",
    "@astrojs/sitemap": "^3.0.5",
    "@astrojs/solid-js": "^4.0.1",
    "@observablehq/plot": "^0.6.13",
    "@solid-primitives/resize-observer": "^2.0.23",
    "@solid-primitives/scheduled": "^1.4.2",
    "astro": "^4.3.2",
    "astro-expressive-code": "^0.32.4",
    "astro-seo": "^0.8.0",
    "axios": "^1.6.7",
    "bulma": "^0.9.4",
    "bulma-divider": "^0.2.0",
    "bulma-pricingtable": "^0.2.0",
    "bulma-steps": "^2.2.1",
    "bulma-switch": "^2.0.4",
    "bulma-tooltip": "^3.0.2",
    "d3": "^7.8.5",
    "htl": "^0.3.1",
    "mermaid": "^10.8.0",
    "slugify": "^1.6.6",
    "solid-js": "^1.8.14",
    "swagger-ui-dist": "^5.11.1",
    "typescript": "^5.3.3",
    "vite-plugin-wasm-pack": "^0.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.5.3",
    "@types/d3": "^7.4.3",
    "hast-util-from-html-isomorphic": "^2.0.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-slug": "^6.0.0",
    "sass": "^1.70.0",
    "vitest": "^1.2.2"
  }
}
