---
export const prerender = false;

import DocsLayout from "../../../layouts/docs/DocsLayout.astro";
import SwaggerPanel from "../../../components/docs/SwaggerPanel";
import { Language } from "../../../i18n/ui";
import Collection from "../../../util/collection";

const collection = Collection.reference;

// Using `meta.env` requires `prerender = false`
const BENCHER_API_URL = import.meta.env.BENCHER_API_URL;
const title = "REST API";
const description = "The REST API for continuous benchmarking with Bencher";
---

<DocsLayout title={title} description={description} collection={collection} slug="api">
  <SwaggerPanel client:only="solid-js" apiUrl={BENCHER_API_URL} lang={Language.en} />
</DocsLayout>
